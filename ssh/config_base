Host *
    # 共通設定
    # クリップボード共有で必要
    ForwardX11 yes
    ForwardX11Trusted yes

Host github.com
    Hostname ssh.github.com
    Port 443
    ProxyCommand nc -X connect -x proxy.cc.utsunomiya-u.ac.jp:8080 %h %p

Host bitbucket.org
    HostName altssh.bitbucket.org
    port 443
    ProxyCommand nc -X connect -x proxy.cc.utsunomiya-u.ac.jp:8080 %h %p
